{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.phimnhalam.website/api/:path*"
    },
    {
      "source": "/movies/:path*",
      "destination": "https://api.phimnhalam.website/movies/:path*"
    },
    {
      "source": "/subtitles/:path*",
      "destination": "https://cdn.phimnhalam.website/subtitles/:path*"
    },
    {
      "source": "/cdn/:path*",
      "destination": "https://cdn.phimnhalam.website/:path*"
    },
    {
      "source": "/ai/:path*",
      "destination": "https://api-facereg.shuneo.com/ai/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
